{
  "schemaVersion": 1,
  "id": "constfly",
  "version": "${version}",
  "name": "ConstFly",
  "description": "An addon that provides an elytra-fly module for Constantiam",
  "authors": [
    "xtendera"
  ],
  "contact": {
    "sources": "https://github.com/Xtendera/ConstFly"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.ashrayshah.constfly.ConstFly"
    ]
  },
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.9"],
    "meteor-client": "*"
  }
}
